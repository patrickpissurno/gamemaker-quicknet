        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Receive and process data
connections_count = qnet_server_get_connections_count();

var data = "0";

do {
    data = qnet_server_queue_poll();
    if(data == "0" || data == "-1") //"0" means empty, "-1" means error
        continue;
    
    var parsed = qnet_parse_data(data);
    var key = parsed[0];
    var value = parsed[1];
    
    show_debug_message("KEY: '" + key + "', VALUE: '" + value + "'");
    
    switch(key)
    {
        case "new_connection":
            // tell the client to create the other players
            var arr = array_create(2);
            arr[0] = o_player;
            for(var i = 0; i &lt; instance_number(o_player); i++)
            {
                arr[1] = instance_find(o_player, i);
                qnet_server_queue_reliable_put_array_int("instance_create", arr);
            }
        
            // create a player for that client
            var player = qnet_server_instance_create(room_width / 2, room_height / 2, o_player);
            player.player_id = real(value) + 1;
            break;
        case "disconnected":
            var player_id = real(value) + 1;
            for(var i = 0; i &lt; instance_number(o_player); i++)
            {
                var player = instance_find(o_player, i);
                if(player.player_id == player_id)
                {
                    qnet_server_queue_reliable_put_int("instance_destroy", player);
                    instance_destroy(player);
                    break;
                }
            }
            break;
    }
}
until(data == "0" || data == "-1");
</string>
          </argument>
